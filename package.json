{
    "name": "virtual-lab-core",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "engines": {
        "node": "18"
    },
    "scripts": {
        "postinstall": "npm run install:client && npm run install:websocket-server",
        "install:client": "cd packages/client && npm install",
        "install:websocket-server": "cd packages/websocket-server && npm install",
        "dev": "AWS_PROFILE=virtual-lab sst dev",
        "build": "sst build",
        "deploy": "sst deploy",
        "remove": "AWS_PROFILE=virtual-lab sst remove",
        "console": "sst console",
        "typecheck": "tsc --noEmit",
        "format": "prettier --write .",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
    },
    "devDependencies": {
        "@aws-sdk/client-ssm": "^3.409.0",
        "@tsconfig/node18": "1.0.3",
        "@types/aws-lambda": "^8.10.119",
        "@types/node": "^18.17.13",
        "@typescript-eslint/eslint-plugin": "^6.6.0",
        "@typescript-eslint/parser": "^6.6.0",
        "aws-cdk-lib": "2.91.0",
        "constructs": "10.2.69",
        "eslint": "^8.48.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "prettier": "^3.0.3",
        "sst": "2.24.25",
        "typescript": "^5.2.2"
    },
    "workspaces": [
        "packages/api"
    ]
}
