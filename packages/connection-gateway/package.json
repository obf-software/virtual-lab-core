{
    "name": "@virtual-lab-core/connection-gateway",
    "version": "0.0.0",
    "engines": {
        "node": "18"
    },
    "scripts": {
        "dev": "npm run dev:build && npm run dev:run",
        "dev:build": "docker build -t connection-gateway .",
        "dev:run": "AWS_PROFILE=virtual-lab npx sst bind \"env | grep -E 'SST_|AWS_|VLC_' > .env.tmp && docker run --env-file .env.tmp -p 8080:8080 connection-gateway\""
    },
    "dependencies": {
        "@aws-sdk/client-eventbridge": "^3.540.0",
        "@aws-sdk/client-ssm": "^3.540.0",
        "bufferutil": "^4.0.7",
        "deep-extend": "^0.6.0",
        "moment": "^2.29.4",
        "utf-8-validate": "^5.0.10",
        "winston": "^3.10.0",
        "ws": "^8.14.1",
        "zod": "^3.22.2"
    }
}
